#include<stdio.h>
void swa0(int *a, int *b)
{
int temp=*a;
*a=*b;
*b=temp;
printf("values after swapping is: \na=%d:\nb=%d\n",*a,*b);
}
void main()
{
int a,b;
int *c, *d;
*c=a; *d=b;
printf("enter any two numbers: \n");
scanf("%d",&a);
scanf("%d",&b);
printf("values before swapping: \na=%d\nb=%d\n",a,b);
}
